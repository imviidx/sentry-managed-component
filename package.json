{
  "name": "sentry-zaraz-consent-integration",
  "version": "1.0.0",
  "description": "Sentry integration with Zaraz consent management",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "demo:dev": "cd demo && npm install && npm run dev",
    "demo:build": "npm run build && cd demo && npm install && npm run build",
    "zaraz-consent": "cd packages/zaraz-consent-tools && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/POD666/sentry-managed-component.git"
  },
  "keywords": [
    "sentry",
    "zaraz",
    "consent",
    "privacy"
  ],
  "author": "POD666",
  "license": "Apache-2.0",
  "peerDependencies": {
    "@sentry/react": "^10.0.0",
    "react": "^18.0.0"
  },
  "devDependencies": {
    "@sentry/types": "^8.55.0",
    "@types/node": "^24.3.0",
    "@types/react": "^18.2.55",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ]
}
